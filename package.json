{
  "name": "unocss-variants",
  "type": "module",
  "version": "1.0.0-alpha.2",
  "packageManager": "pnpm@10.7.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/praburangki/unocss-variants"
  },
  "bugs": {
    "url": "https://github.com/praburangki/unocss-variants/issues"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsup && node copy-types.cjs",
    "prepare": "pnpm simple-git-hooks",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "test": "vitest",
    "update:deps": "pnpx taze major -wIr"
  },
  "dependencies": {
    "@vinicunca/perkakas": "^1.5.0",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/node": "^22.13.13",
    "@vinicunca/eslint-config": "^3.10.0",
    "bumpp": "^10.1.0",
    "clean-package": "^2.2.0",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "simple-git-hooks": "^2.12.1",
    "taze": "^19.0.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "unocss": "^66.1.0-beta.7",
    "vitest": "^3.0.9"
  },
  "tsup": {
    "clean": true,
    "minify": true,
    "treeshake": true,
    "entry": [
      "src/*.js"
    ],
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit ${1}"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
