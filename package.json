{
  "name": "unocss-variants",
  "type": "module",
  "version": "1.0.0-alpha.3",
  "packageManager": "pnpm@10.13.1",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/praburangki/unocss-variants"
  },
  "bugs": {
    "url": "https://github.com/praburangki/unocss-variants/issues"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsdown && node copy-types.cjs",
    "prepare": "pnpm simple-git-hooks",
    "prepack": "clean-package",
    "release": "bumpp -r",
    "postpack": "clean-package restore",
    "test": "vitest",
    "update:deps": "pnpx taze major -wIr"
  },
  "dependencies": {
    "@vinicunca/perkakas": "^1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^24.1.0",
    "@vinicunca/eslint-config": "^3.19.0",
    "bumpp": "^10.2.0",
    "clean-package": "^2.2.0",
    "eslint": "^9.31.0",
    "lint-staged": "^16.1.2",
    "simple-git-hooks": "^2.13.0",
    "taze": "^19.1.0",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "vitest": "^3.2.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit ${1}"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
